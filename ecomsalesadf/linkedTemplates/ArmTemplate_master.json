{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ecomsalesadf"
		},
		"LS_SqlServerTable_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_SqlServerTable'",
			"defaultValue": "Integrated Security=True;Data Source=DESKTOP-S4AIAT2\\SQLEXPRESS;Initial Catalog=salesecom"
		},
		"LS_SqlServerTable_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_SqlServerTable'"
		},
		"LS_SQL_Parquet_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_SQL_Parquet'"
		},
		"LS_SqlServerTable_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "DESKTOP-S4AIAT2\\Madhu"
		},
		"LS_SQL_Parquet_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlssidsalesdev.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ecomsalesadf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_SQL_Parquet_accountKey": {
						"value": "[parameters('LS_SQL_Parquet_accountKey')]"
					},
					"LS_SqlServerTable_connectionString": {
						"value": "[parameters('LS_SqlServerTable_connectionString')]"
					},
					"LS_SqlServerTable_password": {
						"value": "[parameters('LS_SqlServerTable_password')]"
					},
					"LS_SQL_Parquet_properties_typeProperties_url": {
						"value": "[parameters('LS_SQL_Parquet_properties_typeProperties_url')]"
					},
					"LS_SqlServerTable_properties_typeProperties_userName": {
						"value": "[parameters('LS_SqlServerTable_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}